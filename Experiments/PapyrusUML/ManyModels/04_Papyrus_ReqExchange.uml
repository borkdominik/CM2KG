  <Requirements:Requirement xmi:id="_bPmIkGzFEeiXKo7Z7RQZIw" base_Class="_bPjFQGzFEeiXKo7Z7RQZIw" id="03c" text="&quot;Requirement Dateien im (.di, .uml, .notation) Format werden durch den ReqExchange Client unterstützt.&#xA;&quot;"/>
  <Requirements:Requirement xmi:id="_dUy6gGzFEeiXKo7Z7RQZIw" base_Class="_dUvQIGzFEeiXKo7Z7RQZIw" id="03d" text="Microsoft Office Word Dateien im Format .docx werden vom ReqExchange Client unterstützt."/>
  <Requirements:Requirement xmi:id="_wNdCkGzFEeiXKo7Z7RQZIw" base_Class="_wNYxIGzFEeiXKo7Z7RQZIw" id="03e" text="Es soll möglich sein, weitere Formate in die Architektur einzubinden."/>
  <Requirements:Requirement xmi:id="_9KC00GzFEeiXKo7Z7RQZIw" base_Class="_9J31sGzFEeiXKo7Z7RQZIw" id="04" text="Es soll möglich sein, weitere Formate in die Architektur einzubinden."/>
  <Requirements:Requirement xmi:id="_AmoQcGzGEeiXKo7Z7RQZIw" base_Class="_AmkmEGzGEeiXKo7Z7RQZIw" id="04a" text="Der Nutzer kann Änderungen an Requirement Dateien mit Hilfe des ReqExchange Clients an den ReqExchange Server senden. Zuvor werden die Änderungen in das definierte Metamodell überführt."/>
  <Requirements:Requirement xmi:id="_AyVxMGzGEeiXKo7Z7RQZIw" base_Class="_AyMnQGzGEeiXKo7Z7RQZIw" id="04b" text="Der Nutzer kann Änderungen an Requirement Dateien von anderen Nutzern mit Hilfe des ReqExchange Clients vom ReqExchange Server empfangen."/>
  <Requirements:Requirement xmi:id="_A9R14GzGEeiXKo7Z7RQZIw" base_Class="_A9J6EGzGEeiXKo7Z7RQZIw" id="04c1" text="Der Nutzer kann Änderungen an Requirement Dateien von anderen Nutzern mit Hilfe des ReqExchange Clients vom ReqExchange Server empfangen."/>
